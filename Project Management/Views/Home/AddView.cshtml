@model Project_Management.Models.ProjectDashboard


@{
    ViewBag.Title = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@{
    <h2 style="border-bottom:solid 1px #016363">@Session["session_pname"] <i style="color:#016363" class="fa fa-th"></i></h2>

}

<style type="text/css">
    .panel-default {
        border-color: #006F6F;
    }

    .panel {
        border-radius: 0px;
    }

    .panel-default > .panel-heading {
        color: floralwhite;
        background-color: rgba(0, 111, 111, 0.76);
    }

    .panel-heading {
        border-top-left-radius: 0px;
        border-top-right-radius: 0px;
    }

    .chat-panel {
        height: 454px;
    }

    #main_container {
        padding-top: 20px !important;
    }
</style>


<div class="row">
    <div class="col-lg-3 col-md-6">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-3">
                        <i class="fa fa-cube fa-5x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        <div class="huge">@Model.ModuleCount</div>
                        <div>Modules</div>
                    </div>
                </div>
            </div>
            <a style="cursor:pointer" onclick="location.href='@Url.Action("ModuleDashboard", "Home", new { ID = Session["session_pid"] })'">
                <div class="panel-footer">
                    <span class="pull-left">View Details</span>
                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>

    <div class="col-lg-3 col-md-6">
        <div class="panel panel-green" style="background-color: #006F6F; color: floralwhite;">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-3">
                        <i class="fa fa-comments-o fa-5x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        <div class="huge">0</div>
                        <div>Comments</div>
                    </div>
                </div>
            </div>
            <a style="cursor:pointer" onclick="location.href='@Url.Action("AddComment", "Home")'">
                <div class="panel-footer">
                    <span class="pull-left">View Details</span>
                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>

    <div class="col-lg-3 col-md-6">
        <div class="panel panel-yellow" style="border: solid 1px #D44E68;">
            <div class="panel-heading" style="background: #C15A5A; color: floralwhite;">
                <div class="row">
                    <div class="col-xs-3">
                        <i class="fa fa-folder-open fa-5x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        <div class="huge">@Model.FileCount</div>
                        <div>Files</div>
                    </div>
                </div>
            </div>
            <a style="cursor:pointer" onclick="location.href='@Url.Action("FilesGrid", "Home", new { ID = Session["session_pid"] })'">
                <div class="panel-footer">
                    <span class="pull-left">View Details</span>
                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>

    <div class="col-lg-3 col-md-6">
        <div class="panel panel-red" style="border: solid 1px #82845D">
            <div class="panel-heading" style="background: #82845D; color: floralwhite;">
                <div class="row">
                    <div class="col-xs-3">
                        <i class="fa fa-sticky-note fa-5x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        <div id="minutesCount" class="huge">@Model.MomCount</div>
                        <div>Minutes Of Meeting</div>
                    </div>
                </div>
            </div>
            <a style="cursor:pointer" onclick="location.href='@Url.Action("AddMom", "Home")'">
                <div class="panel-footer">
                    <span class="pull-left">View Details</span>
                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>

</div>

<div class="row">

    <div id="gantChartContainer" class="col-lg-8">
        <div class="panel panel-default">
            <div class="panel-heading">
                <i class="fa fa-align-left fa-fw"></i>Gantt Chart
            </div>
            <div class="panel-body" style="height:331px;">
                @{
                    if (Model.Modules.Count() > 0)
                    {
                        @Html.Partial("GenerateGanttChart", "Module1,Module2,Module3")
                    }
                    else
                    {
                        @Html.Partial("NoChartData")
                    }
                 }
            </div>
            <!-- /.panel-body -->
        </div>
    </div>
