            }
        }, 300);

        function DrawDonuts(startDiv, c) {
            var donutsDataArray = new Array();
            var j = 1;
            for (var i = 0; i < CurrDonutsWindow.length; i++) {
                var x = parseInt($(CurrDonutsWindow[i]).attr("data-totaldays"));
                var y;

                var startDate = parseInt($(CurrDonutsWindow[i]).attr("data-startdt"));
                var d = new Date(0);
                d.setUTCMilliseconds(startDate);
                var currDate = new Date();
                currDate = parseInt(currDate.getTime() / 1000) * 1000; //Today's Date
                var compltdDays = parseInt((currDate - startDate) / (1000 * 60 * 60 * 24));
                if (compltdDays < 0)
                    compltdDays = 0;

                var weekday1 = d.getDay();
                var round_days;
                if (weekday1 == 1)
                    round_days = 0;
                else if (weekday1 == 2)
                    round_days = 1;
                else if (weekday1 == 3)
                    round_days = 2;
                else if (weekday1 == 4)
                    round_days = 3;
                else if (weekday1 == 5)
                    round_days = 4;
                var tempDays = compltdDays + round_days;
                compltdDays = compltdDays - (parseInt(tempDays / 7) * 2);
                y = x - compltdDays;  //remaining days
                $("#donutContainer-" + j).css("display", "block");
                if (y < 0) {
                    var data = {
                        chart: {
                            renderTo: 'donutContainer-' + j,
                            type: 'pie'
                        },
                        title: {
                            text: $(CurrDonutsWindow[i]).attr("data-moduleName") + "<br/>Total Days: " + x,
                            align: 'center',
                            verticalAlign: 'middle',
                            style: {
                                fontSize: '12'
                            }
                        },
                        colors: ['#006F6F', '#DDD'],
                        yAxis: {
                            title: {
                                text: 'Total percent market share'
                            }
                        },
                        plotOptions: {
                            pie: {
                                animation: {
                                    duration: 1300,
                                    easing: 'easeOutBounce'
                                },
                                shadow: true
                            }
                        },
                        tooltip: {
                            formatter: function () {
                                return '<b>' + this.point.name + '</b>: ' + "overdue";
                            }
                        },
                        credits: {
                            enabled: false
                        },
                        exporting: {
                            enabled: false
                        },
                        series: [{
                            name: 'Browsers',
                            data: [["Completed Days", compltdDays]],
                            size: '100%',
                            innerSize: '80%',
                            showInLegend: false,
                            dataLabels: {
                                enabled: false
                            }
                        }]
                    };
                }
                else {
                    var data = {
                        chart: {
                            renderTo: 'donutContainer-' + j,
                            type: 'pie'
                        },
                        title: {
                            text: $(CurrDonutsWindow[i]).attr("data-moduleName") + "<br/>Total Days: " + x,
                            align: 'center',
                            verticalAlign: 'middle',
                            style: {
                                fontSize: '12'
                            }
                        },
                        colors: ['#006F6F', '#EEE'],
                        yAxis: {
                            title: {
                                text: 'Total percent market share'
                            }
                        },
                        plotOptions: {
                            pie: {
                                animation: {
                                    duration: 1300,
                                    easing: 'easeOutBounce'
                                },
                                allowPointSelect: true,
                                cursor: 'pointer',
                                shadow: true
                            }
                        },
                        tooltip: {
                            formatter: function () {
                                return '<b>' + this.point.name + '</b>: ' + this.y;
                            }
                        },
                        credits: {
                            enabled: false
                        },
                        exporting: {
                            enabled: false
                        },
                        series: [{
                            name: 'Browsers',
                            data: [["Completed Days", compltdDays], ["Remaining Days",y]],
                            size: '100%',
                            innerSize: '80%',
                            showInLegend: false,
                            dataLabels: {
                                enabled: false
                            }
                        }]
                    };
                }
                j++;
                donutsDataArray.push(data);
            }
            for (var i = 0; i < donutsDataArray.length; i++) {
                new Highcharts.Chart(donutsDataArray[i]);
            }
        }

        Math.easeOutBounce = function (pos) {
            if ((pos) < (1 / 2.75)) {
                return (7.5625 * pos * pos);
            }
            if (pos < (2 / 2.75)) {
                return (7.5625 * (pos -= (1.5 / 2.75)) * pos + 0.75);
            }
            if (pos < (2.5 / 2.75)) {
                return (7.5625 * (pos -= (2.25 / 2.75)) * pos + 0.9375);
            }
            return (7.5625 * (pos -= (2.625 / 2.75)) * pos + 0.984375);
        };

